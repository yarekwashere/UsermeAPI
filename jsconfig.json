{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "es5",
    "strict": true,
    "alwaysStrict": true,
    "noEmit": true,
    "jsx": "react"
  },
  "exclude": ["node_modules", "config", "public", "**/*.test.js", "**/*.test.jsx"],
  "include": ["**/*.js", "**/*.jsx"]
}
